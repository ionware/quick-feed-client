{
  "extends": [
    "airbnb", "prettier"
  ],
  "rules": {
    "prettier/prettier": "error"
  },
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  }
}